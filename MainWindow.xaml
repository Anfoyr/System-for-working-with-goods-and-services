<Window x:Class="WpfApp4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp4"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="900" Icon="Resource/Приятный шелест.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="200*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Background="AliceBlue" Orientation="Horizontal">
            <Image Source="Resource/Приятный шелест.png"
                   HorizontalAlignment="Left" Margin="10 0 0 0"/>
            <Button x:Name="btnBack" Click="btnBack_Click"
                    Width="150" Height="30" FontSize="17"
                    Foreground="White" Background="Aqua"
                    Margin="500 0 0 0" FontFamily="Tahoma">Назад</Button>
            <Button x:Name="btnFaq" Click="btnFaq_Click"
                    Width="150" Height="30" FontSize="17"
                    Foreground="White" Background="Aqua"
                    Margin="5 0 0 0" FontFamily="Tahoma">sff</Button>
        </StackPanel>
        <StackPanel Grid.Row="1" VerticalAlignment="Center">
            <Image Source="Resource/Приятный шелест.png"
                   Opacity=".1"
                   Width="490"/>
        </StackPanel>
        <Frame x:Name="frmMain" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
